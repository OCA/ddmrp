<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">


        <record id="stock_inventory_example" model="stock.inventory">
            <field name="name">Simulating DDMRP Supply Order Generation</field>
        </record>


        <record id="stock_inventory_line_1" model="stock.inventory.line">
            <field name="product_id" ref="product_product_example"/>
            <field name="product_uom_id" ref="product.product_uom_unit"/>
            <field name="inventory_id" ref="stock_inventory_example"/>
            <field name="product_qty">65</field>
            <field name="location_id" ref="stock.stock_location_stock"/>
        </record>

        <function model="stock.inventory" name="action_done">
            <function eval="[[('id', '=', ref('stock_inventory_example'))]]" model="stock.inventory" name="search"/>
        </function>

    </data>
</odoo>
